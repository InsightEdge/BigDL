# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

# Home page
GET         /                                     controllers.WebEndpoint.index

#TODO params
GET         /callSession/:streamedRowId           controllers.CallSessionEndpoint.getLastCallSessions(streamedRowId: String)

POST        /speech                               controllers.KafkaEndpoint.submitSpeech

## Dynamic links
GET         /links/sparkJobs                      controllers.LinksController.sparkJobs()
GET         /links/ieWebUi                        controllers.LinksController.ieWebUi()
GET         /links/tensorFlow                     controllers.LinksController.tensorFlow()


# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file                         controllers.Assets.at(path="/public", file)